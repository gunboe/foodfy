{% extends "layout.njk" %}
 
{% block content %}

<!-- Cards de Receitas -->
<section class="cards">
    {%for item in items%}
    <div class="card" id="{{item.id}}" receita="{{item.card_content}}">
        <img src="/{{item.id}}.png" alt="Imagem de {{item.id}}">
    </div>
    {%endfor%}
</section>

{% endblock %}